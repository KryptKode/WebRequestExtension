{
  "name": "AddLists on Woolworths",
  "manifest_version": 2,
  "description": "Copy list items from one account to another",
  "version": "0.0.1",
  "page_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": ["background/background.js"]
  },
  "permissions": [
    "storage",
    "webRequest",
    "declarativeContent",
    "activeTab",
    "https://www.woolworths.com.au/"
  ]
}
